class Solution(object):
    def twoSum(self, nums, target):
        """
        :type nums: List[int]
        :type target: int
        :rtype: List[int]
        """
       
    for left_idx in range(len(nums)-1):
            right_idx = left_idx+1

            while j < len(nums):
                
                if nums[left_idx] + nums[right_idx] == target:
                    return [left_idx, right_idx]
                
                right_idx+=1
